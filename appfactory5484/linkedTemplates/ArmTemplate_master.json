{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Имя фабрики данных",
			"defaultValue": "appfactory5484"
		},
		"datalake5484_service_accountKey": {
			"type": "secureString",
			"metadata": "Защищенная строка для \"accountKey\" из \"datalake5484_service\""
		},
		"datalake5484_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake5484.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI для учетной записи хранения со связанными шаблонами ARM"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Маркер SAS для учетной записи хранения со связанными шаблонами ARM"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "appfactory5484_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake5484_service_accountKey": {
						"value": "[parameters('datalake5484_service_accountKey')]"
					},
					"datalake5484_service_properties_typeProperties_url": {
						"value": "[parameters('datalake5484_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}